{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "printWidth": 180,
  "proseWrap": "preserve",
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "printWidth": 215
      }
    },
    {
      "files": "*.sql",
      "options": {
        "tabWidth": 4,
        "useTabs": true
      }
    }
  ]
}
